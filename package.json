{
  "name": "porybox",
  "private": true,
  "version": "1.2.3",
  "description": "a Sails application",
  "keywords": [],
  "dependencies": {
    "angular": "1.6.2",
    "angular-animate": "1.6.2",
    "angular-aria": "1.6.2",
    "angular-marked": "1.2.2",
    "angular-material": "1.1.3",
    "angular-messages": "1.6.2",
    "angular-route": "1.6.2",
    "angular-sortable-views": "0.1.0",
    "babel-plugin-lodash": "3.2.11",
    "babel-plugin-transform-async-to-generator": "6.22.0",
    "babel-plugin-transform-exponentiation-operator": "6.22.0",
    "babel-polyfill": "6.23.0",
    "babel-preset-es2015": "6.22.0",
    "babel-register": "6.23.0",
    "babelify": "7.3.0",
    "bcryptjs": "2.4.3",
    "bluebird": "3.4.7",
    "browserify": "14.1.0",
    "clean-css": "4.0.7",
    "clean-css-cli": "4.0.7",
    "connect-mongo": "1.3.2",
    "ejs": "2.5.6",
    "less": "2.7.2",
    "lodash": "4.17.4",
    "moment": "2.17.1",
    "ng-file-upload": "12.2.13",
    "nodemailer": "2.7.2",
    "passport": "0.3.2",
    "passport-http-bearer": "1.0.1",
    "passport-local": "1.0.0",
    "pkparse": "1.1.0",
    "sails": "0.12.11",
    "sails-hook-winston": "1.1.0",
    "sails-mongo": "0.12.2",
    "sails.io.js": "1.1.5",
    "socket.io-client": "1.7.3",
    "uglify-js": "2.7.5",
    "validator": "6.2.1"
  },
  "scripts": {
    "build": "npm run bundle && npm run minify",
    "bundle": "npm run less && browserify client/app.js -o client/bundle.js -t babelify -d",
    "bundle:watch": "watchify client/app.js -o client/bundle.js -t babelify -d -v",
    "less": "lessc client/styles/importer.less client/bundle.css",
    "lint": "eslint ./",
    "minify:js": "uglifyjs client/bundle.js -o client/bundle.min.js -m --screw-ie8",
    "minify:css": "cleancss client/bundle.css -o client/bundle.min.css --s1 --advanced",
    "minify": "npm run minify:js && npm run minify:css",
    "start": "npm run build && node app.js",
    "start:dev": "concurrently \"npm run less\" \"npm run bundle:watch\" \"node app.js\"",
    "test": "npm run lint && npm run test:server && npm run test:client",
    "test:client": "karma start --single-run",
    "test:server": "mocha --timeout 20000 --compilers js:babel-core/register test/bootstrap.test.js test/controller/**/*.js",
    "test:watch": "concurrently \"mocha -w --compilers js:babel-core/register\" \"karma start\""
  },
  "main": "app.js",
  "repository": {
    "type": "git",
    "url": "git@github.com:porybox/porybox.git"
  },
  "author": "Porybox Dev Team",
  "license": "",
  "devDependencies": {
    "angular-mocks": "1.6.2",
    "chai": "3.5.0",
    "concurrently": "3.3.0",
    "dirty-chai": "1.2.2",
    "eslint": "3.15.0",
    "karma": "1.5.0",
    "karma-browserify": "5.1.1",
    "karma-chai": "0.1.0",
    "karma-dirty-chai": "1.0.2",
    "karma-mocha": "1.3.0",
    "karma-phantomjs-launcher": "1.0.2",
    "mocha": "3.2.0",
    "phantomjs-prebuilt": "2.1.14",
    "sails-memory": "0.10.7",
    "sinon": "1.17.7",
    "smtp-tester": "0.6.3",
    "superagent-defaults": "0.1.14",
    "supertest": "3.0.0",
    "supertest-as-promised": "4.0.2",
    "watchify": "3.9.0"
  },
  "greenkeeper": {
    "ignore": [
      "nodemailer"
    ]
  }
}
