    "angular": "1.5.5",
    "angular-animate": "^1.5.0",
    "angular-aria": "^1.5.0",
    "angular-material": "^1.0.7",
    "angular-messages": "^1.5.3",
    "babel-core": "^6.8.0",
    "babel-eslint": "^6.0.2",
    "babel-plugin-transform-async-to-generator": "^6.7.4",
    "babel-polyfill": "^6.7.4",
    "babel-preset-es2015": "^6.6.0",
    "babelify": "^7.2.0",
    "bcryptjs": "^2.3.0",
    "bluebird": "3.4.0",
    "browserify": "13.0.1",
    "concurrently": "^2.1.0",
    "connect-mongo": "^0.8.2",
    "ejs": "2.4.2",
    "eslint": "^2.10.2",
    "grunt": "1.0.1",
    "include-all": "0.1.6",
    "kerberos": "0.0.20",
    "less": "^2.6.0",
    "moment": "^2.11.2",
    "ng-file-upload": "^12.0.4",
    "passport": "^0.3.2",
    "passport-http-bearer": "^1.0.1",
    "passport-local": "^1.0.0",
    "pk6parse": "^0.8.0",
    "rc": "1.0.1",
    "sails": "0.12.3",
    "sails-disk": "0.10.9",
    "sails-hook-winston": "^1.0.0",
    "sails-mongo": "^0.12.0",
    "sails.io.js": "^0.13.7",
    "sinon": "^1.17.3",
    "socket.io-browserify": "^0.9.6",
    "socket.io-client": "^1.4.5",
    "supertest": "^1.2.0",
    "validator": "^5.2.0",
    "watchify": "^3.7.0"
  },
  "scripts": {
    "bundle": "npm run less && browserify client/app.js -o client/bundle.js -t babelify -d",
    "watchbundle": "watchify client/app.js -o client/bundle.js -t babelify -d -v",
    "dev": "concurrent 'npm run less' 'npm run watchbundle' 'node app.js'",
    "frontendTest": "npm run lint && karma start --single-run",
    "less": "lessc client/styles/importer.less client/bundle.css",
    "lint": "eslint ./",
    "start": "npm run bundle && node app.js",
    "test": "npm run lint && mocha --timeout 5000 --compilers js:babel-core/register test/bootstrap.test.js test/controller/**/*.js && karma start --single-run",
    "testwatch": "concurrent 'mocha -w --compilers js:babel-core/register' 'karma start'"
  },
  "main": "app.js",
  "repository": {
    "type": "git",
    "url": "git@github.com:porygonco/porybox.git"
  },
  "author": "PorygonCo",
  "license": "",
  "devDependencies": {
    "angular-mocks": "^1.5.0",
    "chai": "^3.5.0",
    "karma": "^0.13.21",
    "karma-browserify": "^5.0.1",
    "karma-chai": "^0.1.0",
    "karma-mocha": "^1.0.1",
    "karma-phantomjs-launcher": "^1.0.0",
    "mocha": "^2.5.0",
    "phantomjs-prebuilt": "^2.1.4",
    "sails-memory": "^0.10.6",
    "supertest-as-promised": "^3.0.0"
  }
}
